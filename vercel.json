{
  "version": 2,

  "builds": [
    {
      "src": "prototype/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
        {
            "source": "/market-report.html",
            "destination": "/prototype/market-report.html"
        },
        {
            "source": "/daily-summary.html",
            "destination": "/prototype/daily-summary.html"
        },
        {
            "source": "/podcast-player.html",
            "destination": "/prototype/podcast-player.html"
        },
        {
            "source": "/chart-components.js",
            "destination": "/prototype/chart-components.js"
        },
        {
            "source": "/youtube-service.js",
            "destination": "/prototype/youtube-service.js"
        },
        {
            "source": "/",
            "destination": "/prototype/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/prototype/$1"
        }
    ],

  "regions": ["iad1"],
  "env": {},
  "build": {
    "env": {}
  },
  "headers": [
    {
      "source": "/prototype/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/prototype/market-report.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/prototype/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}